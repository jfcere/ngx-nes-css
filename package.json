{
  "name": "ngx-nes-css",
  "version": "0.0.1",
  "description": "Angular library that wraps NES CSS to ease its use within the framework",
  "homepage": "https://github.com/jfcere/ngx-nes-css",
  "license": "MIT",
  "author": {
    "name": "Jean-Francois Cere",
    "email": "jfcere@hotmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jfcere/ngx-nes-css"
  },
  "keywords": [
    "angular",
    "ngx",
    "nes",
    "css",
    "components",
    "retro"
  ],
  "scripts": {
    "ng": "ng",
    "start": "npm run link:lib && ng serve",
    "build:demo": "ng build demo --configuration production",
    "build:lib": "ng build lib --configuration production",
    "postbuild:lib": "cpy ./README.md ./dist/lib && cpy ./LICENSE ./dist/lib",
    "link:lib": "rimraf node_modules/ngx-nes-css && linklocal",
    "lint": "npm run lint:lib && npm run lint:demo",
    "lint:demo": "ng lint demo",
    "lint:lib": "ng lint lib",
    "lint:ci": "ng lint lib --format checkstyle > eslint.xml",
    "type-check:demo": "tsc --project ./demo/tsconfig.app.json --noEmit",
    "type-check:lib": "tsc --project ./lib/tsconfig.lib.json --noEmit",
    "test": "ng test",
    "gh-pages:build": "yarn build:demo --aot --base-href /ngx-nes-css/",
    "gh-pages:deploy": "angular-cli-ghpages --dir=\"dist/demo\" --name=\"Jean-Francois Cere\" --email=\"jfcere@hotmail.com\" --no-silent",
    "publish:lib": "npm publish ./dist/lib"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "ngx-nes-css": "file:./lib",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.2",
    "@angular-eslint/builder": "13.2.0",
    "@angular-eslint/eslint-plugin": "13.2.0",
    "@angular-eslint/eslint-plugin-template": "13.2.0",
    "@angular-eslint/schematics": "13.2.0",
    "@angular-eslint/template-parser": "13.2.0",
    "@angular/cli": "~13.3.2",
    "@angular/compiler-cli": "~13.3.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "angular-cli-ghpages": "^1.0.0",
    "cpy-cli": "^3.1.1",
    "eslint": "^8.12.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "linklocal": "^2.8.2",
    "ng-packagr": "^13.0.0",
    "rimraf": "^3.0.2",
    "typescript": "~4.6.2"
  }
}
